@model TeamFormModel

<h2>Добавяне на отбори</h2>


<form asp-action="CreateMultiple">
        <div class="form-group">
            <label asp-for="TeamCount"></label>
            <input asp-for="TeamCount" class="form-control" required placeholder="Броя на отборите в турнира"/>
        </div>
    <button type="submit" class="btn btn-success">Запази</button>
    <a asp-action="Index" class="btn btn-secondary">Назад</a>
</form>